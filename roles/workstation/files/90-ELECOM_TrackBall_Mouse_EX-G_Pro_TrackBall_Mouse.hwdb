  KEYBOARD_KEY_90007=key_rightshift
  KEYBOARD_KEY_90005=key_f15
  KEYBOARD_KEY_90004=key_f14
  KEYBOARD_KEY_90006=key_rightctrl
  KEYBOARD_KEY_90008=key_f18
