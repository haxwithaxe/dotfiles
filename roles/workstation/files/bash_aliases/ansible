## Ansible Playbook
_argcomplete_with_alias apb ansible-playbook
_argcomplete_with_alias apbl ansible-playbook -i localhost,
_argcomplete_with_alias apbi ansible-playbook -i inventory
# With secrets
_argcomplete_with_alias apbp ansible-playbook --vault-password-file vault-password.secret
_argcomplete_with_alias apblp ansible-playbook --vault-password-file vault-password.secret -i localhost,
_argcomplete_with_alias apbip ansible-playbook -i inventory --vault-password-file vault-password.secret

## Ansible Vault
_argcomplete_with_alias av ansible-vault
# edit
_argcomplete_with_alias ave ansible-vault edit --vault-password-file
_argcomplete_with_alias avep ansible-vault edit --vault-password-file vault-password.secret
# [e]ncrypt
_argcomplete_with_alias avn ansible-vault encrypt --vault-password-file
_argcomplete_with_alias avnp ansible-vault encrypt --vault-password-file vault-password.secret
# [e]ncrypt_string
_argcomplete_with_alias avns ansible-vault encrypt_string --vault-password-file
_argcomplete_with_alias avnsp ansible-vault encrypt --vault-password-file vault-password.secret
# decrypt
_argcomplete_with_alias avd ansible-vault decrypt --vault-password-file
_argcomplete_with_alias avdp ansible-vault decrypt --vault-password-file vault-password.secret
